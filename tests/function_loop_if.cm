int maxOf(int first, int second) {
  if (first > second) {
    return first;
  }
  return second;
}

int accumulateTo(int limit) {
  int result;
  int current;

  result = 0;
  current = 1;
  while (current <= limit) {
    result = result + current;
    current = current + 1;
  }

  return result;
}

void main() {
  int hi;
  int sum;

  hi = maxOf(7, 4);
  sum = accumulateTo(3);

  print(hi);
  print(sum);
}
