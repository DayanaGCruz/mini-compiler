void swap(int arr[], int left, int right) {
  int temp;

  temp = arr[left];
  arr[left] = arr[right];
  arr[right] = temp;
}

void printArray(int arr[], int length) {
  int i;
  char newline;

  newline = '\n';
  i = 0;
  while (i < length) {
    print(arr[i]);
    i = i + 1;
  }
  print(newline);
}

int main() {
  int num[5];
  int idx;
  int length;

  length = 5;
  num[0] = 2;
  num[1] = 4;
  num[2] = 6;
  num[3] = 8;
  num[4] = 10;

  printArray(num, length);

  idx = 0;
  while (idx < length - 1) {
    if (num[idx] < num[idx + 1]) {
      swap(num, idx, idx + 1);
    }
    idx = idx + 1;
  }

  printArray(num, length);

  return 0;
}
