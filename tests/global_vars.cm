/* Regression test for global variables */

int counter;
int data[3];

int bump(int value) {
    counter = counter + value;
    return counter;
}

int main() {
    int i;
    counter = 5;
    data[0] = 7;
    data[1] = 9;
    data[2] = 11;

    i = bump(data[0]);
    output(i);
    i = bump(data[1]);
    output(i);
    i = bump(data[2]);
    output(i);

    return 0;
}
