int add(int lhs, int rhs) {
  int sum;

  sum = lhs + rhs;
  return sum;
}

int fortyTwo() {
  return 42;
}

char echoChar(char input) {
  return input;
}

void main() {
  int total;
  int nested;

  total = add(7, 5);
  print(total);

  nested = add(add(1, 2), fortyTwo());
  print(nested);

  print(echoChar('z'));
}
